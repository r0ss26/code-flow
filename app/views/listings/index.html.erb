<h1>Listings</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Category</th>
      <th>Description</th>
      <th>Price</th>
      <th>Delivery time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <%= render partial: @listings %>
  </tbody>
</table>

<br>

<% if user_signed_in? %>
  <%= link_to 'New Listing', new_listing_path %>
<% end %>
